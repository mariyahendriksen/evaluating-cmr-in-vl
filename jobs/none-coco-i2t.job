#!/bin/sh

DATASET=coco
PERTURBATION=none
TASK=i2t

##################################################################
echo 'evaluation with' $PERTURBATION 'perturbation on' $DATASET
##################################################################
declare -a models=(
                "align"
                "altclip"
                "clip"
                "groupvit"
                )
for MODEL in "${models[@]}"
do
   echo MODEL: $MODEL
   python src/evaluation.py \
    --dataset $DATASET \
    --model $MODEL \
    --perturbation $PERTURBATION \
    --task $TASK
done

echo 'Done!'
