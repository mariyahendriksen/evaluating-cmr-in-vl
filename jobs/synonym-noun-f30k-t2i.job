#!/bin/sh

DATASET=f30k
PERTURBATION=synonym_noun

##################################################################
echo 'evaluation with typos perturbation on f30k'
##################################################################
declare -a models=(
                "align"
                "altclip"
                "clip"
                "groupvit"
                )
for MODEL in "${models[@]}"
do
   echo MODEL: $MODEL
   echo 'evaluation with typos perturbation on f30k'
   python src/evaluation.py \
    --dataset $DATASET \
    --model $MODEL \
    --perturbation $PERTURBATION \
    --task t2i
done

echo 'Done!'
