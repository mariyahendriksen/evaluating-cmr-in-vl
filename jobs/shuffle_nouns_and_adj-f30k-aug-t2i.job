#!/bin/sh

DATASET=f30k_aug
PERTURBATION=shuffle_nouns_and_adj

##################################################################
echo 'evaluation with' $PERTURBATION 'perturbation on' $DATASET
##################################################################
declare -a models=(
                "align"
                "altclip"
                "clip"
                "groupvit"
                )
for MODEL in "${models[@]}"
do
   echo MODEL: $MODEL
   echo 'evaluation with' $PERTURBATION 'perturbation on' $DATASET
   python src/evaluation.py \
    --dataset $DATASET \
    --model $MODEL \
    --perturbation $PERTURBATION \
    --task t2i
done

echo 'Done!'
